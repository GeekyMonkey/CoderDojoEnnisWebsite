{
	"name": "coderdojo-ennis-website",
	"license": "MIT",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "cross-env NODE_OPTIONS=--no-deprecation nuxt build",
		"cf-typegen": "wrangler types",
		"dev": "cls && pnpm install && nuxt dev",
		"deploy": "npm run build && wrangler pages deploy",
		"format": "cls && biome format . --write",
		"generate": "nuxt generate",
		"preview1": "nuxt preview",
		"preview2": "npm run build && wrangler pages dev",
		"postinstall": "nuxt prepare",
		"start": "node .output/server/index.mjs",
		"supabase:sync": "node ../scripts/sync-supabase.js",
		"supabase:commit": "node ../scripts/commit-supabase.js",
		"supabase-migrate": "pnpm supabase:sync && supabase db push --workdir ../supabase && pnpm supabase:commit",
		"supabase-migrate:force": "pnpm supabase:sync && supabase db push --workdir ../supabase --force-db-reset && pnpm supabase:commit",
		"supabase-types": "pnpm supabase:sync && supabase gen types typescript --workdir ../supabase --project-id newsofwpimzmfbqsooka --schema coderdojo > types/supabase.ts",
		"supabase-reset": "pnpm supabase:sync && supabase db reset --workdir ../supabase",
		"test": "cls && vitest"
	},
	"dependencies": {
		"@iconify/json": "2.2.429",
		"@nuxt/ui": "4.3.0",
		"@nuxtjs/google-fonts": "3.2.0",
		"@nuxtjs/i18n": "10.2.1",
		"@nuxtjs/supabase": "2.0.3",
		"@supabase/supabase-js": "2.90.1",
		"@tanstack/vue-query": "5.92.8",
		"@tanstack/vue-table": "^8.21.3",
		"@vueuse/nuxt": "14.1.0",
		"crypto-js": "4.2.0",
		"jose": "6.1.3",
		"mitt": "3.0.1",
		"mssql": "11.0.1",
		"nuxt": "4.2.2",
		"tailwindcss": "4.1.18",
		"vue-qrcode-reader": "5.7.3",
		"zod": "4.3.6"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "4.20260118.0",
		"@nuxt/icon": "2.2.0",
		"@tiptap/core": "3.15.3",
		"@tiptap/pm": "3.15.3",
		"@biomejs/biome": "2.3.12",
		"@types/crypto-js": "4.2.2",
		"@types/jsonwebtoken": "9.0.10",
		"@types/mssql": "9.1.9",
		"@types/node": "24.10.9",
		"@vitejs/plugin-vue": "6.0.3",
		"@vue/test-utils": "2.4.6",
		"c12": "3.3.3",
		"cross-env": "10.1.0",
		"eslint": "9.39.2",
		"nitro-cloudflare-dev": "0.2.2",
		"postcss-custom-properties": "14.0.6",
		"prettier": "3.8.1",
		"prettier-plugin-vue": "1.1.6",
		"sass": "1.97.3",
		"supabase": "2.72.8",
		"ts-node": "10.9.2",
		"typescript": "5.9.3",
		"vite": "6.4.1",
		"vitest": "4.0.18",
		"wrangler": "4.59.2"
	},
	"packageManager": "pnpm@10.28.1",
	"version": "0.1.0"
}
