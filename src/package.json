{
	"name": "coderdojo-ennis-website",
	"license": "MIT",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "cross-env NODE_OPTIONS=--no-deprecation nuxt build",
		"cf-typegen": "wrangler types",
		"dev": "cls && pnpm install && nuxt dev",
		"deploy": "npm run build && wrangler pages deploy",
		"format": "cls && biome format . --write",
		"generate": "nuxt generate",
		"preview1": "nuxt preview",
		"preview2": "npm run build && wrangler pages dev",
		"postinstall": "nuxt prepare",
		"start": "node .output/server/index.mjs",
		"supabase-migrate": "supabase db push",
		"supabase-types": "supabase gen types typescript --project-id newsofwpimzmfbqsooka --schema coderdojo > types/supabase.ts",
		"test": "cls && vitest"
	},
	"dependencies": {
		"@iconify/json": "2.2.405",
		"@nuxt/ui": "4.1.0",
		"@nuxtjs/color-mode": "3.5.2",
		"@nuxtjs/google-fonts": "3.2.0",
		"@nuxtjs/i18n": "10.2.0",
		"@nuxtjs/supabase": "2.0.1",
		"@supabase/supabase-js": "2.80.0",
		"@tanstack/vue-query": "5.90.7",
		"@vueuse/core": "14.0.0",
		"@vueuse/nuxt": "14.0.0",
		"crypto-js": "4.2.0",
		"jose": "6.1.0",
		"mitt": "3.0.1",
		"mssql": "11.0.1",
		"nuxt": "4.2.1",
		"tailwindcss": "4.1.17",
		"vue": "3.5.24",
		"vue-router": "4.6.3",
		"zod": "4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"@cloudflare/workers-types": "4.20251109.0",
		"@nuxt/icon": "2.1.0",
		"@types/crypto-js": "4.2.2",
		"@types/jsonwebtoken": "9.0.10",
		"@types/mssql": "9.1.8",
		"@types/node": "24.10.0",
		"@vitejs/plugin-vue": "6.0.1",
		"@vue/test-utils": "2.4.6",
		"cross-env": "10.1.0",
		"nitro-cloudflare-dev": "0.2.2",
		"postcss-custom-properties": "14.0.6",
		"prettier": "3.6.2",
		"prettier-plugin-vue": "1.1.6",
		"sass": "1.93.3",
		"ts-node": "10.9.2",
		"typescript": "5.9.3",
		"vite": "6.4.1",
		"vitest": "4.0.8",
		"wrangler": "4.46.0"
	},
	"packageManager": "pnpm@10.20.0",
	"prettier": {
		"bracketSameLine": false,
		"endOfLine": "lf",
		"semi": true,
		"singleQuote": false,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true,
		"vueIndentScriptAndStyle": true
	},
	"version": "0.1.0"
}
