{
  "editor.detectIndentation": true,
  "editor.insertSpaces": false,
  "editor.tabSize": 4,
  "files.autoSave": "onFocusChange",
  "files.defaultLanguage": "typescript",
  "files.watcherExclude": {
    "**/node_modules": true,
    "**/bower_components": true
  },
  "peacock.color": "#040d09",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true
  },
  "terminal.integrated.cwd": "${workspaceFolder}/src",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#103424",
    "activityBar.background": "#103424",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#873cc3",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#103424",
    "statusBar.background": "#040d09",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#103424",
    "statusBarItem.remoteBackground": "#040d09",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#040d09",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#040d0999",
    "titleBar.inactiveForeground": "#e7e7e799"
  }
}
