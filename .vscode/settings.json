{
  "editor.codeActionsOnSave": {
    "source.fixAll.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "editor.defaultFormatter": "fronterior.biome-monorepo",
  "editor.detectIndentation": true,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.formatOnSave": true,
  "editor.insertSpaces": false,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "editor.tabSize": 4,
  "files.associations": {
    "*.css": "tailwindcss",
    "*.scss": "tailwindcss"
  },
  "files.autoSave": "onFocusChange",
  "files.defaultLanguage": "typescript",
  "files.watcherExclude": {
    "**/node_modules": true,
    "**/bower_components": true
  },
  "peacock.color": "#215732",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true
  },
  "tailwindCSS.classAttributes": ["class", "ui"],
  "tailwindCSS.experimental.classRegex": [
    ["ui:\\s*{([^)]*)\\s*}", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "terminal.integrated.cwd": "${workspaceFolder}/src",
  "SQL-Formatter-VSCode.dataTypeCase": "upper",
  "SQL-Formatter-VSCode.dialect": "postgresql",
  "SQL-Formatter-VSCode.ignoreTabSettings": true,
  "SQL-Formatter-VSCode.insertSpacesOverride": true,
